    "fig.update_xaxes(title_text=\"Nombre de passagers\", row=2, col=1)\n",
    "fig.update_xaxes(title_text=\"Heure (0-23)\", row=2, col=2)\n",
    "\n",
    "fig.show()\n",
    "\n",
    "# Statistiques descriptives\n",
    "stats_desc = sample_data[['TRIP_DISTANCE', 'FARE_AMOUNT', 'TOTAL_AMOUNT', 'PASSENGER_COUNT']].describe()\n",
    "print(\"\\nüìä Statistiques descriptives (√©chantillon):\")\n",
    "display(stats_desc)"
